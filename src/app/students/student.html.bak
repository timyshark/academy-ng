<div class="panel panel-primary" *ngFor="let student of students" >
    <div class="panel-heading">
        <h3 class="panel-title">{{student.fname}}</h3>
    </div>
    <div class="panel-body">
        <div class="col-xs-10">
            <div class="row">
                <div class="col-xs-4">
                <img [src]="student.picturePath" />
                </div>
                <div class="col-xs-8">
                    <div class="row">
                        <div class="col-xs-6">
                            Last Name
                        </div>
                        <div class="col-xs-6">
                            : {{student.lName}}
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-6">
                            Email Address
                        </div>
                        <div class="col-xs-6">
                            : {{student.sEmail}}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
